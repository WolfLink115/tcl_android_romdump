<!--
This XML file declares which signature|privileged permissions should be granted to privileged
applications on GMS or Google-branded devices.
It allows additional grants on top of privapp-permissions-platform.xml
-->
<permissions>
<privapp-permissions package="com.tct.cs.omadm">
	<permission name="android.permission.ACCESS_NETWORK_STATE"/>
	<permission name="android.permission.DISABLE_KEYGUARD"/>
	<permission name="android.permission.WRITE_EXTERNAL_STORAGE"/>
	<permission name="android.permission.READ_EXTERNAL_STORAGE"/>
	<permission name="android.permission.INTERNET"/>
	<permission name="android.permission.RECEIVE_BOOT_COMPLETED"/>
	<permission name="android.permission.ACTION_REBOOT"/>
	<permission name="android.permission.READ_PHONE_STATE"/>
	<permission name="android.permission.RECEIVE_WAP_PUSH"/>
	<permission name="android.permission.WAKE_LOCK"/>
	<permission name="android.permission.SYSTEM_ALERT_WINDOWN"/>
	<permission name="android.permission.FOREGROUND_SERVICE"/>
	<permission name="android.permission.CLEAR_APP_CACHE"/>
	<permission name="android.permission.RECOVERY"/>
	<permission name="android.permission.REBOOT"/>
	<permission name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
	<permission name="android.permission.GET_PACKAGE_SIZE"/>
	<permission name="android.permission.RECEIVE_SYNCNOTIFY"/>
	<permission name="android.permission.CHANGE_NETWORK_STATE"/>
	<permission name="android.permission.ACCESS_WIFI_STATE"/>
	<permission name="android.permission.WRITE_SETTINGS"/>
	<permission name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
	<permission name="android.permission.LOCAL_MAC_ADDRESS"/>
	<permission name="com.android.launcher.permission.READ_SETTINGS"/>
	<permission name="android.permission.MODIFY_PHONE_STATE"/>
</privapp-permissions>
</permissions>

